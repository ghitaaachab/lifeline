<div class="navbar-custom">
  <div>
    <%= link_to "/", class: "navbar-logo" do %>
      Lifeline
    <% end %>
  </div>
  <div class="navbar-pages">
    <div>Discover Lifeline</div>
    <div>Services</div>
    <div>About</div>
    <div>Contact</div>
  </div>
  <div class="navbar-actions">
    <% if !user_signed_in? %>
        <div> <%= link_to "Sign in", new_user_session_path, class: "btn-signin" %></div>
        <div> <%= link_to "Sign up", new_user_registration_path, class: "btn-signup" %></div>
      <% else %>
        <div> <%= link_to "Dashboard", dashboard_path, class: "btn-signin" %></div>
        <div> <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: "btn-signin" %></div>
    <% end %>
  </div>

</div>
